  "scripts": {
    "dev": "concurrently \"npm run frontend\" \"npm run backend\"",
    "dev:server": "tsx watch server/index.ts",
    "frontend": "vite",
    "backend": "tsx watch server/index.ts",
    "dev:all": "concurrently \"npm run frontend\" \"npm run backend\"",
    "studio": "dotenv -e .env.server -- prisma studio",
    "db:push": "dotenv -e .env.server -- prisma db push",
    "db:pull": "dotenv -e .env.server -- prisma db pull",
    "build": "tsc --project tsconfig.server.json",
    "build:frontend": "vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:all": "npm run build && npm run build:server",
    "start": "tsx server/index.ts",
    "start:prod": "tsx server/index.ts",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "generate:icons": "node scripts/generate-icons.js",
    "validate:env": "node -e \"const fs = require('fs'); const path = require('path'); const envFile = process.env.NODE_ENV === 'production' ? '.env.server' : '.env.development'; if (!fs.existsSync(envFile)) { console.error('‚ùå Fichier manquant:', envFile); process.exit(1); } const required = ['DATABASE_URL', 'JWT_SECRET']; const content = fs.readFileSync(envFile, 'utf8'); const missing = required.filter(k => !content.includes(k + '=')); if (missing.length) { console.error('‚ùå Variables manquantes dans', envFile + ':', missing); process.exit(1); } console.log('‚úÖ Variables environnement valid√©es (' + envFile + ')');\"",
    "pre-deploy": "node pre-deploy-check.js",
    "security:audit": "npm audit && echo '‚úÖ Audit s√©curit√© termin√©' || echo '‚ö†Ô∏è Vuln√©rabilit√©s d√©tect√©es'",
    "check:deps": "npx depcheck --ignores='@types/*,eslint-*,prettier,concurrently' || echo '‚ö†Ô∏è Installer depcheck: npm i -D depcheck'",
    "analyze:bundle": "vite build --mode analyze && echo 'üìä Ouvrir dist/stats.html' || echo '‚ö†Ô∏è Installer vite-bundle-visualizer'"
  },
